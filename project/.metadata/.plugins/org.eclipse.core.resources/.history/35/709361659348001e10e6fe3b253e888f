	public void setUrl(String url) { this.url = url; }
	  
	public String getUserld() { return userId; }
	public void setUserId(String userId) { this.userId = userId; }
	  
	public String getllserPw() { return userPw; }
	public void setllserPw(String userPw) { this.userPw = userPw; }
}
